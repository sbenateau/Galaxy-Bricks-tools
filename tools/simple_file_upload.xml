<tool id="simple_file_upload" name="Uploader des données" version="0.0.1">
    <description>Uploader un fichier de données</description>
    <command detect_errors="exit_code">
        <![CDATA[
            cat '$input_file' > '$output_file'
          ]]>
    </command>
    <inputs>
        <param format="tabular,csv,txt" name="input_file" type="data" label="Fichier à importer" help="csv or tabular" />
    </inputs>
    <outputs>
        <data format="tabular,csv,txt" name="output_file" label="${input_file}" />
    </outputs>
    <help>

    </help>
</tool>
